function multipleOfThreeOrFive(number){
  let finalSum = 0; 
  if (number <= 0) {
    return 0; 
  } else {
    for (let i = 0; i < number; i++) {
      if (i % 5 === 0 && i % 3 === 0) {
        finalSum += i; 
      } else if (i % 5 === 0) {
        finalSum += i; 
      } else if (i % 3 === 0) {
        finalSum += i; 
      }
    }
    return finalSum; 
  }

}